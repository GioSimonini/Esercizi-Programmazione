#include <stdio.h>

int main() {

    int DIM=0,i;
    printf("Inserisci dimensioni vettore ");
    scanf("%d",&DIM);
    int v[DIM];
    
    for (i=0; i<DIM ; i++){
        printf("inserisci valore ");
        scanf("%d", &v[i]);
      }

  for (i=DIM-1; i>=0; i--){
     printf("%d \n", v[i]);
  }
}

