#include <stdio.h>

main(){
  int n,c,i;
  printf("inserisci un numero ");
  scanf("%d",&n);
  i=0;
  c=n;
  printf("%d\n",n);
  
  do{
      if(n%c==0){
        printf("%d  ",c);
        i++;
        }
    c--;
  }while(c>=1);

  if(i==2){
    printf("\n");
    printf("%d",n);
     printf(" Ã© un numero primo");
    }
}
  

