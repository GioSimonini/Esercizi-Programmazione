#include<stdio.h>
#include<string.h>

#define R 4
#define C 10

int main() {
   //in caso di ipossibilit√† di modificare le strighe come ora metto const char *v[R] 
    char *v[R] ={"ciao," ,"come","stai?"};
    int i;
    
    for(i=0; i<R; i++){
        if(v[i] != NULL)
            printf("%d %s\n",i,v[i]);
    }
    
    return 0;
}
