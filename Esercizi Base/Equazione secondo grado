#include Void main() { 
float a, b, c, 

delta, x, x1, x2;

printf(“inserire coefficiente a:”);
scanf(“%f” , &a); 
printf(“inserire coefficiente b:”); 
scanf(“%f” , &b); 
printf(“inserire coefficiente c:”); 
scanf(“%f” , &c); 
delta=b*b-4*a*c; 
if (delta<0) 

printf(“impossibile\n”); 
{  
x=(-b)/(2*a); 	 	
 printf(“una soluzione:%f\n” , x); 	
 } else { 	 	
 x1=(-b+sqrt(delta))/(2*a); 	 	
 x2=(-b-sqrt(delta))/(2*a); 	 	
 printf(“due soluzioni: %f %f\n” , x1, x2); }

 }

